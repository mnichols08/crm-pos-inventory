{
  "name": "crm-pos-inventory",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "setup": "echo \"Getting Firebase SDK config...\" && env-cmd bin/get-firebase-sdk-config.js --file ./config/default.json",
    "check-config": "if [ ! -f ./src/client-config.json ]; then npm run setup; else echo \"Client config already exists, running command...\"; fi;",
    "start": "npm run check-config && env-cmd craco start",
    "start:dist": "npm run build && firebase emulators:start --only hosting",
    "start:emulate": "cross-env NODE_APP_INSTANCE=emulators npm run start",
    "test": "craco test",
    "functions:start": "npm run start --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "firebase emulators:exec --only auth,firestore,database,pubsub \"npm run --prefix functions test:base\"",
    "functions:test:cov": "firebase emulators:exec --only auth,firestore,database,pubsub \"npm run --prefix functions test:cov:base\"",
    "emulators": "firebase emulators:start --only auth,firestore,database,pubsub",
    "emulators:hosting": "firebase emulators:start --only auth,firestore,database,hosting",
    "emulators:all": "firebase emulators:start",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnichols08/crm-pos-inventory.git"
  },
  "engines": {
    "node": ">14.15.0"
  },
  "author": "mnichols08 (https://github.com/mnichols08)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@mui/styles": "^5.2.2",
    "@sentry/browser": "^6.15.0",
    "firebase": "^9.5.0",
    "notistack": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-redux": "^7.1.1",
    "react-redux-firebase": "^3.8.1",
    "react-hook-form": "^7.17.2",
    "react-router-dom": "^5.3.0",
    "redux": "^4.0.4",
    "redux-auth-wrapper": "^2.1.0",
    "redux-firestore": "^0.11.0",
    "redux-thunk": "^2.3.0",
    "stackdriver-errors-js": "^0.12.0"
  },
  "devDependencies": {
    "@craco/craco": "6.3.0",
    "@sentry/cli": "^2.14.3",
    "babel-eslint": "^10.1.0",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "firebase-ci": "^0.15.0",
    "firebase-tools": "^9.23.0",
    "husky": "^8.0.3",
    "lint-staged": "^12.1.2",
    "minimist": "1.2.8",
    "prettier": "^2.5.0",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "port": 3000
  }
}
